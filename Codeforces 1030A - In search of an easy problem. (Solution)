#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    int a[n];
    int c=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]==1)
        c++;
    }
    if(c>0)
    cout<<"Hard"<<endl;
    else cout<<"Easy";
    return 0;
}
